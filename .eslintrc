{
    "$schema": "https://json.schemastore.org/eslintrc",
    "env": {
        "es6": true
    },
    "extends": ["eslint:recommended", "next/core-web-vitals", "prettier"],
    "root": true,
    "rules": {
        "import/no-anonymous-default-export": "off",
        "no-console": "warn",
        "no-control-regex": "off",
        "react-hooks/exhaustive-deps": "off"
    }
}
